<%= button_to(
      'Upvote',
      produce_vouches_path(season.produce),
      {
        params: {
          vouch: {
            value: true,
            user_id: current_user.id,
            season_id: season.id
          }
        },
        class: 'btn btn-success'
      }
    ) %>
<%= button_to(
      'Downvote',
      produce_vouches_path(season.produce),
      {
        params: {
          vouch: {
            value: false,
            user_id: current_user.id,
            season_id: season.id
          }
        },
        class: 'btn btn-danger'
      }
    ) %>
